#module 
#defcfunc shuffle_notepad str s1, local ss, local slist, local n, local index, local r, local s

	ss = s1
	notesel ss
	n = notemax
	repeat n
		noteget slist(cnt),cnt
	loop

	repeat n - 1
		r = rnd(n - cnt) + cnt
		tmp = slist(cnt)
		slist(cnt) = slist(r)
		slist(r) = tmp
	loop

	ss = ""
	repeat n
		noteadd slist(cnt)
	loop
	noteunsel
	return ss
#global

randomize 
mes(shuffle_notepad("a\nb\nc\nd\ne\nf\ng\nh"))